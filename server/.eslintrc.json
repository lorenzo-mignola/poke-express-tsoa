{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {},
  "settings": {
    "import/extensions": [
      ".js",
      ".mjs",
      ".jsx",
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ]
  },
}